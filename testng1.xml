<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd" >

<suite name="Suite1" verbose="1" thread-count="2" parallel="classes">
    <listeners>
        <listener class-name="org.selenium.pom.utils.testrail.listeners.LogSuiteListener"/>
    </listeners>
    <test name="Chrome">
        <parameter name="browser" value="CHROME"/>
        <classes>
            <class name="org.selenium.pom.tests.LoginTest"/>
            <class name="org.selenium.pom.tests.AddToCartTest"/>
        </classes>
    </test>

</suite>
